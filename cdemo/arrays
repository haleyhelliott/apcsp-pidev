#include <stdio.h>

void addArray();
void main()
{
    int arr[100];
    for (int a = 0; a <100; a++)
    {
      arr[a] = a*a;
      printf("%d\n", arr[a]);
    }
    printf(" \n");
    addArray(arr, 100, 1);
}

void addArray(int* arr, int i, int s)
{
    for (int num = 0; num < i; num++)
    {
        arr[num] = arr[num] + s;
        printf("%d\n", arr[num]);
    }
}
